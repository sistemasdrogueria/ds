        $(document).ready(function () {
            var accesoCandidate = window.location.pathname;
            if (accesoCandidate === '/Acceso/' && $(window).width() <= 768) {
                $('.icon.mvlicon').addClass('ocultar');
            }
        });

        $(window).resize(function () {
            var accesoCandidate = window.location.pathname;
            if (accesoCandidate === '/Acceso/' && $(window).width() <= 768) {
                $('.icon.mvlicon').addClass('ocultar');
            }


            if ($(window).width() <= 768 && $('#m_mbl').hasClass('menu_visible')) {
                $('#m_mbl').show();
            } else if ($(window).width() > 768) {
                $('#m_mbl').show();
                $('#m_mbl').removeClass('menu_visible');
                $('#js-navabierto').hide();
            }
            else {
                $('#m_mbl').hide();
            }
        });

        $(":input").on("blur", function (e) {
            this.value = this.value.trim();
        });

        function CheckAdVisibility(sectionId, isGrandparentContained) {
            setTimeout(function () {
                var section = $("#" + sectionId);
                var sectionVisible = section.css("display") != 'none';
                var sectionEmpty = section.is(":empty");
                var sectionContainer = section.parent();
                if (isGrandparentContained) { sectionContainer = sectionContainer.parent(); }

                if (sectionContainer.css("display") == 'none') {
                    if (sectionVisible && !sectionEmpty) { sectionContainer.show(); }
                }
            }, 15000);
        }
    </script>

    
