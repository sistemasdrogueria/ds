<script>

    const $emailInput = $("#LoginModel_Email");
    const $passwordInput = $("#LoginModel_Password");

    const $divLogin = $("#divUserLogin");
    const $divRegister = $("#registerContainer");
    const $divRememberPassword = $("#rememberPasswordContainer");

    const $effectClasses = "rightIn rightOut leftIn leftOut";
    const $effLeftIn = "leftIn";
    const $effLeftOut = "leftOut";
    const $effRightIn = "rightIn";
    const $effRightOut = "rightOut";

    let emailSubmit = false;

    $(document).ready(function () {
        if (window.location.hash && window.location.hash == '#_=_') {
            window.location.hash = '';
        }


        $emailInput.rules("remove", "emailexists");

        CancelTabButtonOnKeyDown();

        $("input").bind('keypress', function (event) {
            if (event.keyCode === 13) {
                event.preventDefault();
                switch (this.id) {
                    case "LoginModel_Email":
                        $("#continueWithMailButton").click();
                        break;
                    case "LoginModel_Password":
                        $("#btnSubmitPass").click();
                        break;
                    case "RememberPasswordModel_Email":
                        $("#sbRemember").click();
                        break;
                }
            }
        });

        $("#continueWithMailButton").on("click", function () {
            emailSubmit = true;
            const valid = ValidateEmailInput();
            if (valid) {
                GetUserRegistered($emailInput.val());
            }
        });

        $("#continueWithMailButton").on("change", function () {
            if (emailSubmit) {
                ValidateEmailInput();
            }
        });

        $("#rememberPasswordContinueButton").on("click", function () {
            $("#LoginModel_Password").removeClass("hide");
            $('#rememberPasswordPhotoContainer').toggleClass('hide');
            ShowRememberPasswordToLoginAnimation();
        });

        $("#showRememberPasswordButton").on("click", function () {
            $('#rememberPasswordPhotoContainer').toggleClass('hide');
            ShowLoginToRememberPasswordAnimation();
        });

        $("#showLoginFormButton").on("click", function () {
            $('#rememberPasswordPhotoContainer').toggleClass('hide');
            ShowRememberPasswordToLoginAnimation();
        });

        $("#returnRegisterButton").on("click", function () {
            ShowLoginToRegisterAnimation();
            ResetPhotoAndName();
        });

        $("#returnLoginButton").on("click", function () {
            $('#rememberPasswordPhotoContainer').toggleClass('hide');
            ShowRememberPasswordToLoginAnimation();
        });
    });

    function ValidateEmailInput() {
        return $emailInput.valid();
    }

    function ValidatePasswordInput() {
        return $passwordInput.valid();
    }

    function GetUserRegistered(email) {
            const welcomeMaleLit = 'Bienvenido de nuevo';
            const welcomeFemaleLit = 'Bienvenida de nuevo';
            $.ajax({
                url: '/ajax/getuserregistered',
                type: 'GET',
                dataType: 'json',
                data: { 'email': email },
                beforeSend: function () {
                    $("#continueWithMailButton").addClass("disabled");
                },
                success: function (data) {
                    if (data.Name) {
                        //Login
                        if (data.Photo) {
                            $('[photo]').attr('src', 'https://ii.ct-stc.com/5/' + data.Photo);
                            $('[photo-container]').removeClass("hide");
                            $('[no-photo-container]').addClass("hide");
                        }
                        $('[user-name-text]').text((data.Gender === 1 ? welcomeMaleLit : welcomeFemaleLit) + ' ' + data.Name);
                        $("#AccessModel_Email").val(email);
                        ShowRegisterToLoginAnimation();
                        $("#LoginModel_Name").val(data.Name);
                        $("#LoginModel_PhotoPath").val(data.Photo);
                        $("#LoginModel_Gender").val(data.Gender);
                        $("#RememberPasswordModel_Email").val(email);
                        $("#RememberPasswordModel_Name").val(data.Name);
                        $("#RememberPasswordModel_PhotoPath").val(data.Photo);
                        $("#RememberPasswordModel_Gender").val(data.Gender);
                    } else {
                        if (data.idstatus == 5) {
                            //Enviar a la home
                            location.href = data.url;
                        }
                        else {
                            //Registro
                            location.href = `/Step1?m=` + data.email + ``;
                        }
                    }
                },
                error: function (ex) {
                    console.error(ex.statusText);
                },
                complete: function () {
                    $("#continueWithMailButton").removeClass("disabled");
                }
            });
    }
        var typoDTO = {
            "message": "¿Querías decir"
    };

    document.querySelectorAll("[login-google-popup]").forEach(function (element) {
        element.addEventListener("click", function () {
		var coo = getCookie('uca');
            if (!coo) {
                var deviceIphone = "iphone",
                    deviceIpad = "ipad",
                    deviceIpod = "ipod",
                    uagent = navigator.userAgent.toLowerCase(),
                    isApple = uagent.search(deviceIphone) > -1 || uagent.search(deviceIpod) > -1 || uagent.search(deviceIpad) > -1;
                if (isApple) {
                    window.location.assign("/Google/RedirectToGoogle");
                } else {
                    window.popupGoogle = window.open('/Google/RedirectToGoogle?fpp=true', 'popupGoogle', 'width=600,height=600,scrollbars=no,resizable=no');
                    checkLoginGoogle = setInterval(checkPopUpGoogle, 1000);
                }
            } else {
                window.location.reload();
            }
        });
    });

    function checkPopUpGoogle() {
        if (window.getCookie('uca') != "" || window.getCookie('gulr') != "") {
            clearInterval(checkLoginGoogle);

            if (window.getCookie('gulr') != "") {
                let urlRedirect = window.getCookie('gulr');
                if (urlRedirect) {
                    setCookie('gulr', '', -1);
                    window.location.href = urlRedirect;
                }
            } else {
                window.location.reload();
            }
        }
    }

    function ShowRegisterToLoginAnimation() {
        ReplaceShowClass($divLogin, $effLeftIn);
        ReplaceShowClass($divRegister, $effRightOut);
    }

    function ShowLoginToRegisterAnimation() {
        ReplaceShowClass($divRegister, $effRightIn);
        ReplaceShowClass($divLogin, $effLeftOut);
    }

    function ShowRememberPasswordToLoginAnimation() {
        ReplaceShowClass($divRememberPassword, $effLeftOut);
        ReplaceShowClass($divLogin, $effLeftIn);
    }

    function ShowLoginToRememberPasswordAnimation() {
        ReplaceShowClass($divRememberPassword, $effLeftIn);
        ReplaceShowClass($divLogin, $effRightOut);
    }

    function ShowRegisterToRememberPasswordAnimation() {
        $('#rememberPasswordPhotoContainer').removeClass('hide');
        ReplaceShowClass($divRememberPassword, $effLeftIn);
        ReplaceShowClass($divRegister, $effRightOut);
        ReplaceShowClass($divLogin, $effRightOut);
    }

    function ReplaceShowClass($elem, newClass) {
        $elem.removeClass($effectClasses);
        $elem.addClass(newClass);
    }

    function ResetPhotoAndName() {
        $('[photo]').attr('src', '');
        $('[photo-container]').addClass("hide");
        $('[no-photo-container]').removeClass("hide");
        $('[user-name-text]').text('');
    }

    function CancelTabButtonOnKeyDown() {
        document.onkeydown = function (t) {
            if (t.which === 9) {
                return false;
            }
        }
    }
</script>


